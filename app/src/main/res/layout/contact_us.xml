<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app2="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="+216 11 111 111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView18"
        android:textSize="14sp"
        android:lineSpacingExtra="14sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textAlignment="viewStart"
        android:fontFamily="casual"
        android:layout_marginLeft="46dp"
        android:layout_marginStart="46dp"
        android:layout_alignBaseline="@+id/textView17"
        android:layout_alignBottom="@+id/textView17"
        android:layout_toRightOf="@+id/textView17"
        android:layout_toEndOf="@+id/textView17" />

    <TextView
        android:text="Phone :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView17"
        android:layout_marginTop="45dp"
        android:textSize="14sp"
        android:lineSpacingExtra="14sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textAlignment="viewStart"
        android:fontFamily="casual"
        android:layout_below="@+id/textView13"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13" />

    <TextView
        android:text="Contact us on :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:layout_marginTop="46dp"
        android:textSize="30sp"
        android:lineSpacingExtra="14sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textAlignment="viewStart"
        android:fontFamily="casual"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="34dp"
        android:layout_marginStart="34dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app2:srcCompat="@drawable/call"
        android:id="@+id/imageButton"
        android:layout_below="@+id/textView13"
        android:layout_toRightOf="@+id/textView12"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="29dp"
        android:onClick="Call" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app2:srcCompat="@drawable/m"
        android:id="@+id/imageButton7"
        android:layout_below="@+id/imageButton"
        android:layout_alignLeft="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton"
        android:layout_marginTop="12dp"
        android:onClick="SMS" />

    <TextView
        android:text="+216 11 111 111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView12"
        android:textSize="14sp"
        android:lineSpacingExtra="14sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textAlignment="viewStart"
        android:fontFamily="casual"
        android:layout_alignTop="@+id/imageButton7"
        android:layout_alignLeft="@+id/textView18"
        android:layout_alignStart="@+id/textView18"
        android:layout_marginTop="12dp" />

    <TextView
        android:text="SMS :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:textSize="14sp"
        android:lineSpacingExtra="14sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textAlignment="viewStart"
        android:fontFamily="casual"
        android:layout_alignBaseline="@+id/textView12"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton7"
        android:layout_alignLeft="@+id/textView19"
        android:layout_alignStart="@+id/textView19"
        android:layout_marginTop="28dp"
        android:id="@+id/button2"
        android:text="Find us on map"
        android:onClick="Findus" />


</RelativeLayout>